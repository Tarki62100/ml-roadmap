   "source": [
    "\n",
    "import numpy as np\n",
    "def mean(x):\n",
    "    total= 0\n",
    "    for i in x:\n",
    "        total+= i\n",
    "    return total/len(x)\n",
    "\n",
    "def midmean(x):\n",
    "    sum(x)/len(x)\n",
    "\n",
    "arr = np.random.random(10000000)\n",
    "\n"
   ]
  },
